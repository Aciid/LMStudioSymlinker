{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "swift: Build Debug LMStudioSymlinker",
      "type": "shell",
      "command": "swift build",
      "options": {
        "cwd": "${workspaceFolder}",
        "env": {}
      },
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "presentation": {
        "reveal": "silent",
        "panel": "shared"
      }
    },
    {
      "label": "swift: Build Release LMStudioSymlinker",
      "type": "shell",
      "command": "swift build -c release",
      "options": {
        "cwd": "${workspaceFolder}",
        "env": {}
      },
      "group": "build",
      "presentation": {
        "reveal": "silent",
        "panel": "shared"
      }
    },
    {
      "label": "swift: Build (no pre-launch)",
      "type": "shell",
      "command": "swift build",
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "presentation": {
        "reveal": "silent",
        "panel": "shared"
      }
    }
  ]
}
